{
	"name": "my-svelte-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "bun run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint . --cache",
		"deploy": "bun run build && wrangler deploy",
		"cf-typegen": "wrangler types src/worker-configuration.d.ts",
		"auth:generate": "bun x @better-auth/cli generate --config src/lib/server/auth/better-auth.config.ts --output src/lib/schema/auth.schema.ts -y",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"d1:migrate": "wrangler d1 migrations apply PUSSADU_DB",
		"d1:migrate:remote": "wrangler d1 migrations apply PUSSADU_DB --remote",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build",
		"storybook:new": "hygen component new",
		"storybook:add": "hygen component add"
	},
	"devDependencies": {
		"@better-auth/cli": "^1.4.17",
		"@chromatic-com/storybook": "^4.1.3",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.561.0",
		"@storybook/addon-a11y": "^9.1.17",
		"@storybook/addon-docs": "^9.1.17",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-vitest": "^9.1.17",
		"@storybook/sveltekit": "^9.1.17",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.5",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/table-core": "^8.21.3",
		"@types/node": "^22.19.7",
		"@vitest/browser": "^3.2.4",
		"clsx": "^2.1.1",
		"cross-env": "^10.1.0",
