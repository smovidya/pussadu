		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "^9.1.17",
		"eslint-plugin-svelte": "^3.14.0",
		"formsnap": "^2.0.1",
		"globals": "^16.5.0",
		"layerchart": "2.0.0-next.27",
		"lefthook": "^1.13.6",
		"mode-watcher": "^1.1.0",
		"paneforge": "^1.0.2",
		"playwright": "^1.57.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"storybook": "^9.1.17",
